              <a href="tel:{{ settings.header_phone }}">
                {% if settings.usecustomphoneicon == true %}
                  <img src="{{ 'phone-icon.png' | asset_img_url: '60x' }}" class="picon" alt="Phone Icon" width="60" height="60" loading="lazy" />
