<div class="container  header-space">
  {% if section.settings.show_breadcrumb %}
  {% render 'breadcrumb' %}
{% endif %}
  <div class="grid">
    <div class="grid__item large--one-whole page--item">
      <h1>{{ page.title }}</h1>
      <div class="rte">
        {{ page.content }}
      </div>
    </div>
  </div>
</div>
{% schema %}
  {
    "name": "Page",
    "settings": []
  }
{% endschema %}
