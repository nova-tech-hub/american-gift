<div class="page-header-main  header-space">
  <div class="page-header-container">
    <div>
      <h1 class="page-heading">{{ page.title }}</h1>
      
      {% render 'breadcrumb' %}
    </div>
  </div>


<div class="container  header-space">
  <div class="page-content-box">
    <div class="rte content-section">
        <div class="page-content">


  <div class="full-width-container wishlist-container">
    <!-- Hero Banner -->
  
    <!-- Product Tile Grid -->
    <div class="wishlist-grid flex" style="display: none">
      {% paginate collections.all.products by 1000 %}
      {% for product in collections.all.products  %}
        {% render 'product-grid-item' with product: product %}
      {% endfor %}
        {% endpaginate %}
      <div class="wishlist-grid--empty-list flex">
        <div class="empty-list--info">
          <h6 class="empty-list--text black">Looks like you haven't started a wishlist yet...</h6>
          <a class="btn empty-list--btn" href="/collections/all">Start Shopping</a>
        </div>
      </div>
    </div>
    <!-- Loader -->
    <div class="wishlist-loader">
      <p class="wishlist-loader--text">Loading your customized wishlist...</p>
    </div>

  </div>

   </div>
      </div>
  </div>

  
</div>
</div>
<style>
  html {
    overflow-x: hidden;
  }
</style>
{% schema %}
  {
    "name": "Wishlist Page",
    "settings": []
  }
{% endschema %}